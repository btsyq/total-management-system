.pop-box{
    margin: 0;
    padding: 0;
}
.pop-box{
    width:860px;
    height:600px;
    padding:0 40px;
    margin: 0 auto;
    background: url(../img/pop-bg.png);
    background-size:100% 100%;
    position: relative;
    display: block;
}
.pop-title{
    height:32.5px;
    padding: 12.5px 0 0 0;
    font-size: 12px;
}
.pop-close{
    width:13px;
    height:13px;
    background: url(../img/close.png);
    background-size:100% 100%;
    position: absolute;
    top:20px;
    right:20px;
    cursor: pointer;
}
.pub-btn{
    width:90px;
    height:25px;
    line-height:25px;
    text-align: center;
    color:#5dd9fe;
    cursor: pointer;
}
.pre-resources{
    display: none;
}
.pre-construction{
    display: none;
}
.summarize{
    background: url(../img/role_sum.png);
    background-size:100% 100%;
    color:#fcd200;
    /*margin-top:8.5px;*/
}
.resources{
    background: url(../img/role-sum2.png);
    background-size:100% 100%;
    margin-left:20px;
}
.construction{
    background: url(../img/role-sum2.png);
    background-size:100% 100%;
    margin-left:20px;
}
.popover{
    height:90%;
}
.pop-left{
    width:48.5%;
}
.pop-right{
    width:48.5%;
}
.basic-message{
    color:#00fcff;
    font-size:14px;
    padding: 15px 0 8px 0;
}
/*表格样式*/
.table-fixed {
    border: 1px solid #276eab;
    border-radius:5px;
    table-layout: fixed;
    width: 100%;
	font-size:12px;
    color:#fff;
}
.table-roll tbody{
	display: block;
}
.table-roll tr{
	display: block;
}
.data-news{
    height:116px;
}
.set-message{
    padding-top: 50px;
}
.tab-news{
    border: 1px solid #276eab;
    border-radius:5px;
    table-layout: fixed;
    width: 100%;
	font-size:12px;
    color:#fff;
    text-align: center;
    margin-top:54px;
}
.tab-news tr{
    color:#aeecff;
    height:38px;
    line-height: 38px;
    border-bottom: 1px solid #276eab;
}
.tab-news th{
   border-right: 1px solid #276eab; 
}
.tab-news td{
   border-right: 1px solid #276eab; 
}
