
.conleft{
	float: left;
	width: 260px;
	height: 565px;
	box-sizing: border-box;
	padding-right: 18px;
}
.conright{
	float: left;
	width: calc(100% - 260px);
	height: 100%;
}
.cltit{
	height: 48px;
	background: rgba(22,141,253,.3);
	border: 1px solid #2467a7;
}
.clmenu{
	margin-top: 14px;
	height: 565px;
	background: rgba(22,141,253,.3);
	border: 1px solid #2467a7;
}
.crtit{
	height: 48px;
	background: rgba(22,141,253,.3);
	border: 1px solid #2467a7;	
}
.crmain{
	height: 518px;
	background: rgba(22,141,253,.3);
	border: 1px solid #2467a7;
	/*overflow-y: auto;*/
}
.cltit>p{
	padding: 0 8px;
	line-height: 16px;
	float: left;
	margin-top: 16px;
	margin-left: 20px;
	border-left: 3px solid #e69709;
}
.crtit .move{
	width: 120px;
	height: 30px;
	float: left;
	border-radius: 6px;
	border: 1px solid #5db0fe;
	outline: none;
	color: #fff;
	line-height: 30px;
	background: #1a79d3;	
	/*background: linear-gradient(to bottom,#2a87de,#066ac9);*/
	position: relative;
	text-indent:10px;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
}
.bmor{
	height: 14px;
	width: 30px;
	position: absolute;
	right:3px;
	top:9px;
	text-align: center;
	line-height: 17px;
}
.menutab{
	box-sizing: border-box;
}
.menutab ul li{
	height:47px;
	line-height:47px;
	padding-left:20px;
	border-bottom:1px solid #5db0fe;
	cursor: pointer;
}
.menutab ul li a{
	color:#fff;
}
.menutab ul li:hover{
	background: #168dfd;
}
.btnname{
	line-height: 30px;
	margin: 9px 0 0 22px;
	font-size: 14px;
	position: relative;
}
.btnname input{
	width:140px;
	height:30px;
	background: #04244a;
	border: 1px solid #2e76b9;
    border-radius: 6px;
	color:#fff;
	text-indent: 10px;
}
.btnname select{
	width:140px;
	height:30px;
}
.search-news{
	margin:9px 20px 0 0;
}
.search-news button{
	width:80px;
	height:30px;
	background: #0070d9;
	border-radius:5px;
	border:none;
	color:#fff;
	cursor: pointer;
	margin-left: 10px;
}
.search-news button:nth-child(2){
	background: #019475;
}
.crsync{
	height:47px;
	margin-top:14px;
}
.crsync button{
	width:100px;
	height:30px;
	border-radius:5px;
	background: #d98f00;
	color:#fff;
	border:none;
	cursor: pointer;
}
.sort{
	margin-left:5px;
	cursor: pointer;
}
.addconfirm{
	display: none;
}
.iwleft{
	width: 70px;
	height: 100%;
	float: left;
	font-size: 14px;
	text-align: left;
}
.iwleft>p{
	line-height: 54px;
}
.iwright{
	width: 210px;
	float: left;
	height: 100%;
}
.iwright>p{
	line-height: 54px;
}
.iwright>p>input{
	width: 100%;
	background: none;
	height: 26px;
	line-height: 26px;
	text-indent: 10px;
	color: #fff;
	border: 1px solid #5db0fe;
	border-radius: 5px;
}
.iwright>p>select{
	width: 100%;
	
	height: 26px;
	line-height: 26px;
	text-indent: 10px;
	color: #fff;
	border: 1px solid #5db0fe;
	border-radius: 5px;
	
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	 background: url(../img/select.png) no-repeat scroll right center transparent;
}
select::-ms-expand { display: none; }
.iwright>p>select>option{
	background: #024589;
}
.compile{
	height:129px;
}
.pop-up{
	height:244px;
}
.inputrole{
	padding:40px 40px;
}
.allot{
	height:488px;
}
.pallot{
	height:371px;
	max-height:371px;
	overflow-y: auto;
}
.privileges{
	position: absolute;
	margin-top: -244px;
}
.addrole{
	height:350px;
}
.juese{
	height:230px !important;
}
.addpart input{
	width:15px;
	height:15px;
	background-position: -14px 0;
}
.addpart span{
	height:40px;
	display: block;
    float: left;
    width: 24%;
	clear: both;
}
