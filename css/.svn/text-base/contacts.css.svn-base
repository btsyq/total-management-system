.move>.movegroup{
	position: absolute;
	top: 30px;
	left: 0;
	width: 100%;
	z-index: 1;
	display: none;
}
.movegroup:before{
	content: '';
	position: absolute;
	left: 56px;
	top: 11px;
	width: 10px;
	height: 10px;
	background: #ccc;
	z-index: 2;
	transform: rotate(45deg);
	border-left: 1px solid #4792dc;
	border-top: 1px solid #4792dc;
	background: #124f8f;
}
.movegroup>ul{
	display: block;
	width: 100%;
	font-size: 14px;
	margin-top: 16px;
	border: 1px solid #4792dc;
	border-bottom: none;
	max-height: 400px;
	overflow-y: auto;
	cursor: auto;
}
.movegroup>ul>li{
	display: block;
	box-sizing: border-box;
	padding: 12px 12px;
	line-height: 1.2em;
	border-bottom: 1px solid #4792dc;
	background: #124f8f;
}
.movegroup>ul>li>span{
	display: inline-block;
	width: 68px;
	height: 18px;
	line-height: 20px;
	text-overflow: ellipsis; 
	white-space: nowrap;
	overflow: hidden; 
}
.movegroup>button{
	width: 122px;
	height: 40px;
	border: 1px solid #4792dc;
	background: #1474d0;
	color: #fff;
	border-top: none;
	cursor: pointer;
}

