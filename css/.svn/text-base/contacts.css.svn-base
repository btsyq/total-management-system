	background: #ce4545;
	position: relative;
}
.crtit .move>.movebtn{
	width: 100%;
	height: 100%;
	cursor: pointer;
}
.movebtn>.btxt{
	float: left;
	height: 14px;
	width: 88px;
	line-height: 14px;
	margin-top: 8px;
	font-size: 14px;
	text-align: center;
	border-right: 2px solid #a83131;
}
.movebtn>.bmor{
	float: left;
	height: 14px;
	width: 30px;
