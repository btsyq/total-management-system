/*checkbox样式*/
.mycheck {
  display: inline-block;
  width: 14px;
  height: 14px;
  position: relative;
}
.mycheck input[type=checkbox] {
  visibility: hidden;
}
.mycheck label {
  cursor: pointer;
  position: absolute;
  width: 12px;
  height: 12px;
  top: 4px;
  left: 0;
  border: 1px solid #438afa;
  border-radius: 4px;
}
.mycheck label:after {
  opacity: 0;
  content: '';
  position: absolute;
  width: 7px;
  height: 4px;
  background: transparent;
  top: 2px;
  left: 2px;
  border: 1px solid #d2d2d2;
  border-top: none;
  border-right: none;
  -webkit-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
          transform: rotate(-45deg);
}
.mycheck label:hover::after {
  opacity: 1;
}
.mycheck input[type=checkbox]:checked + label:after {
  opacity: 1;
  border-color: #fff;
}
.mycheck input[type=checkbox]:checked + label {
  background: #0091f0;
  border-color: #0091f0;
}

