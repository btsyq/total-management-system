body,h1,h2,h3,h4,h5,h6,div,p,dl,dt,dd,ol,ul,li,form,table,th,td,a,img,span,strong,var,em,input,textarea,select,option{margin: 0; padding: 0;}
html,body{font-family:"微软雅黑",Arail,Tabhoma; font-size: 16px; text-align: left; height: 100%;width: 100%;}
ul,ol{list-style: none;}
img{border: 0;}
input,select,textarea{outline:0;}
textarea{resize:none;}
table{border-collapse: collapse; border-spacing: 0;}
th,strong,var,em{font-weight: normal; font-style: normal;}
a{text-decoration: none;}

.fl{
	float: left;
}
.fr{
	float: right;
}
.cl{
	clear: both;
}

body{
	margin:0;
	padding:0;
}
.page-news{
    padding:0 25px;
    font-size:14px;
}
.business-news{
    margin: 30px 0;
    font-size:17px;
}
.business-message{
    padding: 0 40px;
}
.bus-name,.bus-time{
    width:15%;
    line-height: 30px;
}
.business-top{
    width:100%;
    height: 60px;
}
.bus-time{
    margin-left: 10%;
}
.business-top input{
    width:312px;
    height:30px;
}
.policymaker{
    margin-top: 77px;
}
.linkman{
    width:15%;
}
.linkman-news{
    width:85%;
}
.linkman-details{
    width:100%;
}
.linkman-details:nth-child(2){
    margin-top:35px;
}
.linkman-news span:nth-child(1){
    display: block;
    width:8%;
}
.linkman-news span:nth-child(2){
    display: block;
    width:15%;
}
.linkman-news span:nth-child(3){
    display: block;
    width:20%;
}
.demand{
    width:57.5%;
    height:130px;
    line-height: 25px;
}
.guidance{
    margin-top: 90px;
}
.scheme input{
    width:20px;
    height:20px;
}
.fou{
    margin-left: 20px;
}
.interflow{
    width:30%;
    height:30px;
}
.business-money span{
    line-height: 30px;
    margin: 0 10px;
}
.probability input{
    width:30%;
}
.business-need{
    height:155px;
}
.table-fixed {
    border: 1px solid #ccc;
    table-layout: fixed;
    width: 57.5%;
}
.table-fixed th {
    width:13%;
    height: 38px;
    border: 1px solid #ccc;
    text-align: center;
}
.table-fixed td {
    border: 1px solid #ccc;
    text-align: left;
    padding: 0 15px;
}
.print{
    margin-top: 60px;
    text-align: center;
}
.print button{
    width:280px;
    height:40px;
    line-height: 40px;
    border-radius: 7px;
    border: 1px solid #666;
    cursor: pointer;
}